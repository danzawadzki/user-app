		"@types/react": "16.4.18",
		"@types/react-dom": "16.0.9",
      "@types/react-redux": "^6.0.9",
      "@types/redux": "^3.6.0",
      "@types/redux-thunk": "^2.1.0",
		"enzyme": "^3.7.0",
		"enzyme-adapter-react-16": "^1.7.0",
		"husky": "^1.1.3",
